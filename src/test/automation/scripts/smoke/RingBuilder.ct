
#Ringbuilder

Save value 'Wellsley Ring' to 'saved.TYPE_OF_RING';

Open page 'urls.MAIN_URL:loginData.CLIENT_USER:loginData.CLIENT_PASSWORD';

Optional Click on item 'generalPage.CLOSE_MODAL_DIALOG';

Click on item 'mainPage.BUILD_YOU_RING_BUTTON';

# Random ring
Click on item 'engagementPage.RING_PRODUCT_ITEM:saved.TYPE_OF_RING';

Fill data form 'smokeData.RING_BUILDER_EXAMPLE_DATA_BY_VALUE';

Click on item 'engagementPage.CHOSE_THIS_SETTING_BUTTON';

Save entity 'diamondsPage.DIAMONDS_TABLE_STRUCTURE' to 'saved.Diamond' by position '10';

Optional Click on item 'generalPage.CLOSE_MODAL_DIALOG';
Optional Click on item 'generalPage.CLOSE_CHAT_DIALOG';

Click on item 'diamondsPage.VIEW_DIAMOND_BY_PARAMETERS_LOCATOR:saved.Diamond.shape:saved.Diamond.weight:saved.Diamond.color:saved.Diamond.clarity:saved.Diamond.grade:saved.Diamond.price';
Optional Click on item 'generalPage.CLOSE_CHAT_DIALOG';

Click on item 'diamondsPage.ADD_TO_RING_BUTTON';

Optional Click on item 'generalPage.CLOSE_CHAT_DIALOG';
Optional Click on item 'generalPage.CLOSE_MODAL_DIALOG';

Click on item 'engagementPage.ADD_TO_BAG_BUTTON';
Optional Click on item 'generalPage.CLOSE_CHAT_DIALOG';
Optional Click on item 'generalPage.CLOSE_MODAL_DIALOG';
Optional Click on item 'engagementPage.ADD_TO_BAG_BUTTON';

Click on item 'generalPage.CHECKOUT_BUTTON';
Wait for '20' seconds;
Validate element 'generalPage.LOADING_IMAGE' is invisible;

Fill data form 'smokeData.DEFAULT_CHECKOUT_DATA';
Type value 'smokeData.DEFAULT_CHECKOUT_DATA.Email' to 'generalPage.CHECKOUT_USERNAME_FIELD';
Type value 'smokeData.DEFAULT_CHECKOUT_DATA.StreetAddress' to 'generalPage.CHECKOUT_STREET_ADDRESS_FIELD';

Wait for '3' seconds;

Click on item 'generalPage.NEXT_BUTTON_CHECKOUT_PAGE';
Wait for '20' seconds;

Click on item 'generalPage.CREDIT_CARD_PAYMENT_OPTION';

Type value 'general.DEFAULT_VISA_CARD_NUMBER' to 'generalPage.CREDIT_CARD_NUMBER';
Type value 'general.DEFAULT_VISA_CARD_EXP_MON' to 'generalPage.CREDIT_CARD_EXP_MON';
Type value 'general.DEFAULT_VISA_CARD_EXP_YEAR' to 'generalPage.CREDIT_CARD_EXP_YEAR';
Type value 'general.DEFAULT_VISA_CARD_CVV' to 'generalPage.CREDIT_CARD_CVV';

Click on item 'generalPage.PLACE_ORDER_BUTTON';

#//h1 Thank you for your purchase!

Save element text 'generalPage.ORDER_NUMBER_ELEMENT' to 'SAVED.orderNumber';
Report message "Order number" "SAVED.orderNumber"


